{
  "kind": "singleType",
  "collectionName": "event_pages",
  "info": {
    "singularName": "event-page",
    "pluralName": "event-pages",
    "displayName": "event-page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pageTitle": {
      "type": "string"
    },
    "heroTitle": {
      "type": "string"
    },
    "heroSubtitle": {
      "type": "string"
    },
    "heroImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featuredEvent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event"
    },
    "upcomingEvents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event"
    }
  }
}
