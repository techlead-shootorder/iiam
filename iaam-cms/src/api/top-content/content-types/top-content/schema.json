{
  "kind": "collectionType",
  "collectionName": "top_contents",
  "info": {
    "singularName": "top-content",
    "pluralName": "top-contents",
    "displayName": "Top Content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "PageTitle": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "PageTitle"
    },
    "BannerImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "BannerHeading": {
      "type": "string"
    },
    "BannerDescription": {
      "type": "text"
    },
    "Section": {
      "type": "component",
      "component": "section.section",
      "repeatable": true
    }
  }
}
